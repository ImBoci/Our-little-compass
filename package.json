{
  "name": "whattocook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build --webpack",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate-sql": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > migration.sql",
    "db:push-turso": "npx tsx scripts/push-schema-to-turso.ts",
    "deploy:db": "npm run db:generate-sql && npm run db:push-turso"
  },
